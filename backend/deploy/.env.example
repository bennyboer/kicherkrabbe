# Domain configuration
DOMAIN=kicherkrabbe.example.com
ACME_EMAIL=admin@example.com

# Traefik dashboard (generate with: htpasswd -nb admin yourpassword)
TRAEFIK_DASHBOARD_AUTH=admin:$$apr1$$...

# MongoDB credentials
MONGO_ROOT_USERNAME=admin
MONGO_ROOT_PASSWORD=change-me-root-password
MONGO_APP_USERNAME=kicherkrabbe
MONGO_APP_PASSWORD=change-me-app-password

# RabbitMQ credentials
RABBITMQ_USER=kicherkrabbe
RABBITMQ_PASSWORD=change-me-rabbitmq-password

# Application image (after building and pushing)
APP_IMAGE=your-registry/kicherkrabbe-server:latest
